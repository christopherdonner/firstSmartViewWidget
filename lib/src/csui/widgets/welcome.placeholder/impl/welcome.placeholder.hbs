
<div class="csui-hero-tile">
  <div class="csui-greeting">{{greeting}}</div>

  {{#if includeVideo}}
  <button class="csui-videoButton" aria-label="{{videoLabel}}"></button>
  {{/if}}
  {{#if includeMessage}}
  <div class="csui-message">
    <p title="{{message}}">{{message}}</p>
  </div>
  {{/if}}

</div>
